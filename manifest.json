{
  "manifest_version": 2,
  "name": "Keep Playing Pandora",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.pandora.com/*",
        "http://www.pandora.com/*"
      ],
      "js": ["pandora.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
